<%= semantic_form_for(@attendance, :url => session_attendance_path(@session)) do |f| %>
  <p>Thanks for your interest in attending this session. Please create an account so we can track who's coming.</p>

  <%= f.inputs :name, :email, :password %>
  <%= f.actions do %>
    <%= f.button 'Create Account', :class => 'button'  %>
  <% end %>
<% end %>
